{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test": {},
    "clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": ["^build", "build"]
    },
    "//#lint": {},
    "//#lint:fix": {
      "cache": false
    }
  }
}
